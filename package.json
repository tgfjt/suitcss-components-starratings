{
  "name": "@tgfjt/suitcss-components-starratings",
  "description": "SUIT CSS component for star ratings",
  "version": "1.0.1",
  "style": "index.css",
  "files": [
    "index.css",
    "index.js",
    "lib"
  ],
  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "suitcss-components-test": "*",
    "suitcss-preprocessor": "^2.0.0"
  },
  "scripts": {
    "build": "npm run setup && npm run preprocess",
    "build-test": "npm run setup && npm run preprocess-test",
    "preprocess": "suitcss --import-root lib index.css build/build.css",
    "preprocess-test": "suitcss --import-root lib test/test.css build/test.css",
    "setup": "npm install && mkdir -p build",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tgfjt/suitcss-components-starratings.git"
  },
  "license": "MIT",
  "keywords": [
    "star",
    "rating",
    "suitcss",
    "style"
  ]
}
